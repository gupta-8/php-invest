{
    "name": "matthiastraka/phpportfoliomanager",
    "type": "project",
    "description": "PHP Stock market portfolio manager",
    "keywords": ["php", "portfolio", "stock"],
    "license": "GPL",
    "authors": [
        {
            "name": "Matthias Straka"
        }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "optimize-autoloader": true,
        "discard-changes": true
    },
    "require": {
        "php": ">=7.2.5",
        "ext-dom": "*",
        "ext-xml": "*",
        "ext-gd": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "symfony/dotenv": "^5.3",
        "symfony/error-handler": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
